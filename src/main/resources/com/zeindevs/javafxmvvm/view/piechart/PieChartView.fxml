<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="400.0" prefWidth="600.0" spacing="8.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.zeindevs.javafxmvvm.view.piechart.PieChartController">
    <children>
        <Label fx:id="eventLabel" text="Label" />
        <Button mnemonicParsing="false" onAction="#onUpdateButton" text="Update" />
        <PieChart fx:id="pieChart" prefHeight="350" prefWidth="400" />
    </children>
   <padding>
      <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
   </padding>
</VBox>
